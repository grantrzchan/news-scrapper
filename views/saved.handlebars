<div class="jumbotron text-center">
    <div class="overlay">
    </div>
    <div class="background-image">
    </div>
    <div class="caption">
        <h1>Your Saved Articles</h1>
        <h4>From the Onion Scrapper</h4>
    </div>
</div>
{{!-- div for all articles; articles take all 12 columns --}}
<div class="container article-container">
    {{#each scraper}}{{#if this.isSaved}}
    <div class="panel panel-default" data-_id={{this._id}}>
        <div class="panel-heading">
            <h3>
                <a class="article-link" href={{this.url}}>{{this.title}}</a>
            </h3>
            <button class="btn btn-danger unsave">Delete Saved Article</button>
        </div>
        <div class="panel-body">
            <p>{{this.note}}</p>
        </div>
    </div>
    {{/if}}{{/each}}
</div>